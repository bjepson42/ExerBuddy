
<p id="notice"><%= notice %></p>
<div class="jumbotron">
<h1>Users</h1>
<table class="table table-hover">
  <thead>
    <%= stylesheet_link_tag "users" %>
    <tr>
      <th>User Name</th>
      <th>Name</th>
      <th>Age</th>
      <th>Gender</th>
      <th>Email</th>
      <th>Phone</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= link_to user.username, user %></td>
        <td><%= user.name %></td>
        <td><%= user.age %></td>
        <td><%= user.gender %></td>
        <td><%= user.email %></td>
        <td><%= user.phone %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<br>

</div>
